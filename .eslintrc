{
    "extends": [
        "airbnb",
        "airbnb/hooks",
        "prettier"
    ],
    "globals": {
        "fetch": "readonly",
        "document": "readonly",
        "window": "readonly"
    },
    "rules": {
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ]
    }
}